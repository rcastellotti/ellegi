{
    "name": "livegrep",
    "fs_paths": [
        {
            "name": "livegrep/livegrep",
            "path": "src/",
            "metadata": {
                "url-pattern": "https://github.com/{name}/blob/HEAD/src/{path}#L{lno}"
            }
        }
    ],
    "repositories": [
        {
            "name": "livegrep",
            "path": ".",
            "revisions": [ "HEAD" ],
            "metadata": {
                "github": "livegrep/livegrep"
            }
        }
    ]
}
